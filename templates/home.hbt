<!doctype html>
<html lang="en">
<head>
  <meta charset="UTF-8" />
  <title>{{#title}}{{.}} |{{/title}} Austin Hyde</title>
  <link href="/css/main.css" rel="stylesheet" type="text/css">
</head>
<body>
{{>header}}
<div class="contents">
{{{contents}}}
<hr>
<h3>Recent Posts</h3>
<dl class="post-list">
{{#each (lookup collections 'posts')}}
  <dt><a href="{{link path}}">{{title}}</a></dt>
  <dd>{{{excerpt}}}</dd>
{{/each}}
</dl>
</div>
</div>
{{>footer}}
</body>
</html>